{
  "expo": {
    "name": "AccessLanka",
    "slug": "accesslanka",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2E7D32"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.accesslanka.app"
    },
    "android": {
      "package": "com.accesslanka.app"
    },
    "web": {
    },
    "plugins": [
      "expo-location",
      "expo-asset"
    ]
  }
}
